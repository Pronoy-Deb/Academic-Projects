<Demo>:
    MDScreen:
        name:"fk"

        MDBoxLayout:
            orientation:"vertical"
            spacing:10

            MDLabel:
                text: "Buy coupon"
                theme_text_color: "Custom"
                text_color: 0,0,0,1
                bold:True
                font_size:20
                pos_hint:{"center_x":.5,"center_y":.5}
                halign:"center"
                size_hint_y:.2

            MDBoxLayout:
                orientation: "horizontal"
                size_hint_x: .7
                size_hint_y:.8
                pos_hint:{"center_x":.5,"center_y":.5}
                MDBoxLayout:
                    orientation:"vertical"
                    
                    spacing: 10
                    MDTextField:
                        id: Sid
                        hint_text: "Enter Student ID"
                        mode:"fill"
                        size_hint: 0.9, None
                        pos_hint: {"center_x":.5}
                    MDTextField:
                        id:SD
                        hint_text: "Starting Date"
                        mode:"fill"
                        size_hint: 0.9, None
                        pos_hint: {"center_x":.5}
                        
                    MDTextField: 
                        id:ED
                        hint_text: "Ending Date"
                        mode: "fill"
                        size_hint: 0.9, None
                        pos_hint: {"center_x":.5}
                        
                    MDTextField: 
                        id: Amount
                        hint_text: "Enter Amount"
                        mode: "fill"
                        size_hint: 0.9, None
                        pos_hint: {"center_x":.5}
                    MDTextField: 
                        id: pdatehis
                        hint_text: "Payment Date"
                        mode: "fill"
                        size_hint: 0.9, None
                        pos_hint: {"center_x":.5}
                        
                    MDBoxLayout:
                        orientation: 'horizontal'
                        spacing: 10
                        padding_x:dp(30)
                        padding_y:dp(20)
                        size_hint: 0.7, 0.3
                        pos_hint:{"center_x":.5,"center_y":.5}
                        MDRaisedButton:
                            text: "Submit"
                            size_hint: 0.1,0.3
                            pos_hint:{"center_x":.5,"center_y":.5}
                            on_press:root.registerUser()
                        MDRaisedButton:
                            text: "Go To Admin Panel"
                            size_hint: 0.1,0.3
                            pos_hint:{"center_x":.5,"center_y":.5}
                            on_press:root.registerUser()
           
                        


            


            
            